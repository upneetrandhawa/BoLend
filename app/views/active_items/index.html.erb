<p id="notice"><%= notice %></p>

<h1>Active Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @active_items.each do |active_item| %>
      <tr>
        <td><%= link_to 'Show', active_item %></td>
        <td><%= link_to 'Edit', edit_active_item_path(active_item) %></td>
        <td><%= link_to 'Destroy', active_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Active Item', new_active_item_path %>
